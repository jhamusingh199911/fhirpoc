pipeline
{
agent any
stages{
stage('Build Application'){
steps{
bat 'mvn clean install'
}
}

stage('Test') {
 steps {
 bat "mvn test"
 }
 }

stage('SonarQube code quality check'){
steps{
bat 'mvn sonar:sonar -Dsonar.sources=src/ -Dsonar.host.url=http://localhost:9000 -Dsonar.login=42e11efc8ae21eca776f60046cb8f4b64b2d1937'
}
}





stage('Deploy Application to Mulesoft Cloudhub'){
steps{
bat 'mvn package deploy -DmuleDeploy'
}
}
}
}